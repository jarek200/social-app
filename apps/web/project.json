{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "web",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro dev",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "astro build",
        "cwd": "apps/web"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx biome lint apps/web"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest --config apps/web/vitest.config.ts"
      }
    }
  },
  "tags": [
    "scope:web",
    "type:application"
  ]
}
